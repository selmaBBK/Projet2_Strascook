<!doctype html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}- Strascook{% endblock %}</title>

    <link rel="stylesheet" href="/assets/css/style.css">
    {% block stylesheet %}
    <!-- UIkit CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/uikit@3.6.19/dist/css/uikit.min.css" />
    {% endblock %}
    <link rel="icon" href="/assets/images/favicon.png">
</head>

<body>
    {% block navbar %}
    {% include '/layout/navBar.html.twig' %}
    {% endblock %}
    
    {% block content %}{% endblock %}
    {% block scroll %}
    <div class="uk-flex uk-flex-right uk-margin-medium-right">
        <a href="" uk-totop uk-scroll data-uk-smooth-scroll width="40" height="40"></a>
    </div>
    {% endblock %}
    {% block footer %}
    {% include '/layout/footer.html.twig' %}
    {% endblock %}

    {% block javascript %}
    <!-- UIkit CSS -->
    <script src="https://cdn.jsdelivr.net/npm/uikit@3.6.19/dist/js/uikit.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/uikit@3.6.19/dist/js/uikit-icons.min.js"></script>
    {% endblock %}
</body>
</html>


